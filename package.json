{
  "name": "bridge-troll",
  "version": "1.0.0",
  "description": "A friendly troll that living under Ontario's bridges",
  "scripts": {
    "generate-bridge-json": "node tools/csv2json.js",
    "build": "parcel build src/index.html",
    "eslint": "eslint tools src test",
    "prettier": "prettier --single-quote --write '{tools,src,test}/**/*.{js,css}'",
    "check-prettier": "prettier --single-quote --list-different '{tools,src,test}/**/*.{js,css}'",
    "lint": "npm run eslint && npm run check-prettier",
    "test": "npm run lint && npm run karma-headless",
    "karma-headless": "karma start karma.conf.js --browsers ChromeHeadless",
    "karma-firefox": "karma start karma.conf.js --browsers Firefox",
    "karma-chrome": "karma start karma.conf.js --browsers Chrome",
    "karma-edge": "karma start karma.conf.js --browsers Edge",
    "start": "parcel src/index.html",
    "debug": "cross-env FAKE_GEO=1 parcel src/index.html"
  },
  "author": "humphd",
  "license": "MIT",
  "devDependencies": {
    "JSONStream": "^1.3.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browserify": "^16.1.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "csv": "^2.0.0",
    "eslint": "^4.18.2",
    "eslint-plugin-chai-assert-bdd": "0.0.3",
    "eslint-plugin-mocha": "^4.12.1",
    "karma": "^2.0.0",
    "karma-browserify": "^5.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^5.0.4",
    "parcel-bundler": "^1.6.2",
    "prettier": "^1.11.1"
  },
  "dependencies": {
    "geo-tree": "^1.0.5",
    "leaflet": "^1.3.1",
    "loglevel": "^1.6.1"
  }
}
